{
  "name": "@sbp/sbp",
  "version": "1.1.0",
  "description": "Selector Basic Programming: JavaScript Edition",
  "main": "dist/main.cjs",
  "type": "module",
  "exports": {
    "import": "./dist/module.mjs",
    "require": "./dist/main.cjs"
  },
  "homepage": "https://github.com/okTurtles/sbp-js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel index.js -o dist/main.cjs && flow-remove-types -p -o dist/module.mjs index.js",
    "flow": "flow",
    "lint": "eslint",
    "clean": "rm -f dist/*"
  },
  "browserslist": "> 0.25% and not dead",
  "author": "Greg Slepak",
  "license": "AGPL-3.0",
  "eslintConfig": {
    "root": true,
    "parser": "@babel/eslint-parser",
    "plugins": [
      "flowtype"
    ],
    "extends": [
      "plugin:flowtype/recommended",
      "standard"
    ]
  },
  "eslintIgnore": [
    "dist/*",
    "node_modules/*",
    "**/*.md"
  ],
  "devDependencies": {
    "@babel/cli": "7.17.0",
    "@babel/core": "7.17.0",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-flow": "7.16.7",
    "eslint-config-standard": "17.0.0-0",
    "eslint-plugin-flowtype": "8.0.3",
    "flow-bin": "0.170.0",
    "flow-remove-types": "2.171.0"
  }
}
